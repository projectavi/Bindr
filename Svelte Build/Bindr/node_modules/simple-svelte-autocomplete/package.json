  "repository": {
    "type": "git",
    "url": "git+https://github.com/pstanoev/simple-svelte-autocomplete"
  },
  "keywords": [
    "svelte",
    "autocomplete",
    "svelte3",
    "svelte-components",
    "autocomplete",
    "auto complete",
    "autosuggest",
    "auto suggest",
    "typeahead",
    "type ahead"
  ],
  "files": [
    "src",
    "index.mjs",
    "index.js"
  ]
}
